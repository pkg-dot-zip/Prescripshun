<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="PrescripshunGui.Views.ProfileView"
             xmlns:vm="clr-namespace:PrescripshunGui.ViewModels"
             x:DataType="vm:ProfileViewModel">
    <StackPanel>
        <TextBlock Text="{Binding Profile.FullName}" FontSize="20" FontWeight="Bold" />
        <TextBlock Text="{Binding Profile.BirthDate}" />
        <TextBlock Text="" />
        <TextBlock Text="{Binding MedicalFile.Diagnoses}" />
        <TextBlock Text="{Binding MedicalFile.Medication}" />
        <TextBlock Text="{Binding MedicalFile.Appointments}" />
        <TextBlock Text="{Binding MedicalFile.Notes}" />
    </StackPanel>
</UserControl>